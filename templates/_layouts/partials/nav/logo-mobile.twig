<a href="#">
    {% if logo %}
        <img
                src="{{ logo.url({height: 32}) }}"
                class="h-12 w-auto hover:opacity-75"
                alt="{{ siteInfo.title }}"
        />
    {% else %}
        <div class="text-2xl text-foreground-dark font-bold">
            {{ siteInfo.title }}
        </div>
    {% endif %}
</a>